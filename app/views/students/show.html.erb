<h1><%= @dojo.branch %>'s - <%= @student.first_name %> <%= @student.last_name %></h1>
<br>
<h2>Email: <%= @student.email %></h2>
<br>
<h3>COHORT</h3>
<br>
<table>
	<thead>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
	</thead>
	<tbody>
    <% @students.each do |student| %>
		<tr>
			<td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.email %></td>
		</tr>
    <% end %>
	</tbody>
</table>
<br>
<br>
<a href="/dojos/<%=@dojo.id%>/students/<%=@student.id%>/edit">Edit</a>
<br><br>
<a href="/dojos/<%=@dojo.id%>">Cohort Page</a>
